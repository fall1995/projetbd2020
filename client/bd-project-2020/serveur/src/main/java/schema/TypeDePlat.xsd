<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://classesGen/typeDePlat"
            xmlns="http://classesGen/typeDePlat"
            elementFormDefault="qualified">
    <xsd:element name="typeDePlat" type="TypeDePlat"/> 
    
    <xsd:simpleType name="TypeDePlat">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Entree"/>
            <xsd:enumeration value="Plat"/>
            <xsd:enumeration value="Dessert"/>
            <xsd:enumeration value="Boisson"/>
            <xsd:enumeration value="Hors-d'oeuvre"/>
        </xsd:restriction>
    </xsd:simpleType>
    
</xsd:schema>
